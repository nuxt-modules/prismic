{
  "name": "@nuxtjs/prismic",
  "version": "1.5.0-alpha.0",
  "description": "Easily connect your Nuxt.js application to your content hosted on Prismic",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "prismic",
    "cms"
  ],
  "repository": "nuxt-modules/prismic",
  "license": "MIT",
  "main": "src/module.js",
  "files": [
    "src",
    "templates"
  ],
  "scripts": {
    "dev": "nuxt example",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "release:dry": "standard-version --dry-run",
    "release:alpha": "yarn test && standard-version --release-as minor --prerelease alpha && git push --follow-tags && npm publish --tag alpha",
    "release:alpha:dry": "standard-version --release-as minor --prerelease alpha --dry-run",
    "unit": "jest",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "npm run lint && npm run unit"
  },
  "dependencies": {
    "@prismicio/client": "^5.1.1 || ^4.0.0",
    "@prismicio/vue": "^2.2.0",
    "@slicemachine/manager": "0.1.1-dev-plugins.3",
    "consola": "^2.15.3",
    "io-ts-types": "^0.5.19",
    "prismic-dom": "^2.2.7"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "latest",
    "@prismicio/slice-simulator-vue": "^0.2.3",
    "@types/jest": "latest",
    "babel-eslint": "latest",
    "eslint": "latest",
    "jest": "latest",
    "nuxt-edge": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= v12.0.0"
  }
}
